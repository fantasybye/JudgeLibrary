{
  "presets": ["es2015", "react", "stage-0"],
  "plugins": [
    "add-module-exports",
    "transform-runtime",
    "transform-es3-property-literals",
    "transform-es3-member-expression-literals",
   // ["transform-es2015-classes", { "loose": true }],
    "transform-proto-to-assign",
    ["import", { "libraryName": "antd"}]
  ]
}

